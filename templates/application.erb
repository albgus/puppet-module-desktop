
[Desktop Entry]
Name=<%= @title_real %>
<% if @comment %>Comment=<%= @comment %><% end %>
<% if @icon %>Icon=<%= @icon %><% end %>
Exec=<%= @exec %>
<% if @path %>Path=<%= @path %><% end %>
<% if @terminal %>Terminal=<%= @terminal %><% else %>Terminal=False<% end %>
Type=Application
Categories=<%= @categories %>
<% if @mime_type %>MimeType=<%= @mime_type %><% end %>
